{{#if model}}
  <div class="pipeline--list">
    {{#if lastClicked}}
    <p>Last slide clicked: <span class="selected-action">{{lastClicked}}</span></p>
    {{/if}}
    {{#md-card-collapsible}}
      {{#each model as |pipeline|}}
        {{#md-collapsible icon=pipeline title=pipeline.title active=(eq pipeline.id activeItem) actionArg=pipeline.id action="choosePipeline"}}
          {{#if editable}}
            {{pipeline-manager pipeline=pipeline activePipeline=(eq lastClicked pipeline.id)}}
          {{else}}
            {{pipeline-list-item pipeline=pipeline}}
          {{/if}}
          {{!-- <div class="row">
            <button type="button" class='btn' name="button" {{action 'manage'}}> {{manageButtonText}}</button>
          </div> --}}
        {{/md-collapsible}}
      {{/each}}
    {{/md-card-collapsible}}
  </div>
{{else}}
  {{#md-card-panel}}
    There are no pipelines yet. Launch one in the
    {{#link-to 'list'}}stacks {{/link-to}}
    section!
  {{/md-card-panel}}
{{/if}}
